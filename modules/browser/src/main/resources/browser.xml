<module>
    <pattern name="BrowserSite">
        <pattern value="*.*"/>
    </pattern>

    <command id="cmd.browser.open">
        <pattern value="[* [откр*|запус*|покажи*|показ*|выйди*|выйти]] * (сайт*|site) $BrowserSite"/>
        <pattern value="* (откр*|запус*|покажи*|показ*|выйди*|выйти) * [на] [сайт*|site] $BrowserSite"/>
        <pattern value="*.*"/>
    </command>
</module>